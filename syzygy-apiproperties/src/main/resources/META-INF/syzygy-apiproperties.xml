<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="apiPublicationManager" class="no.api.properties.publication.PublicationManager"/>

    <bean id="apiPropertiesManager" class="no.api.syzygy.apiproperties.SyzygyApiProperties"
          factory-method="fetchInstanceWith">
        <constructor-arg ref="apiPublicationManager"/>
    </bean>
</beans>
